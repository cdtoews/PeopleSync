<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="[%-5level] SCHEMA=$${ctx:current_schema} - %c{1}.%M - %msg%n" />
		</Console>
		
		 
	</Appenders>
	 <loggers>     
    <root level="trace">
      <appender-ref ref="Console" level="TRACE"/>
      
    </root>    
  </loggers>
</Configuration>